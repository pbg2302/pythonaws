{
    "Statement": [
      {
        "Sid": "Access-to-specific-bucket-only",
        "Principal": "*",
        "Action": [
            "s3:GetObject",
            "s3:PutObject"
        ],
        "Effect": "Allow",
        "Resource": ["arn:aws:s3:::YOUR_BUCKET_NAME",
                     "arn:aws:s3:::YOUR_BUCKET_NAME/*"]
      }
    ]
 }